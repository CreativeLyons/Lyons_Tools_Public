 onCreate "n = nuke.thisNode()\nn\[\"aspectratio\"].setFlag(0x00000004)\nn\[\"switchBokeh\"].setFlag(0x00000008)\n"
 knobChanged "n = nuke.thisNode()\nk = nuke.thisKnob()\n\nif k.name() in \[\"switchBokeh\", \"sizePreset\"]:\n    \n    if k.name() == \"switchBokeh\":\n        with n:\n            s = nuke.toNode(\"Switch1\")\n            wh = nuke.toNode(\"widthHeight\")\n            w = str(int(wh.knob('size_wh').getValue(0)))\n            h = str(int(wh.knob('size_wh').getValue(1)))\n            sw = s.knob('which')\n            sn = int(sw.getValue())\n            \n            rn = s.input(sn).name()\n            r = nuke.toNode(rn)\n            f = nuke.filename(r)\n            ftail = f.split(\"/\").pop().split(\".\")\[0].split(\"Iris_\")\[1]\n            b = ftail + \" \" + \"(\" + w + \"x\" + h + \")\"\n        n.knob(\"bokehName\").setValue(b)\n        n.knob(\"label\").setValue(b)\n    \n    if k.name() == \"sizePreset\":\n        sp = int(n.knob(\"sizePreset\").value())\n        n.knob('size').setValue(sp)"
 tile_color 0x8c8c8cff
 label "Moon (269x269)"
 note_font "DejaVu Sans"
 note_font_color 0xffffffff
 selected true
 xpos -67159
 ypos 948
 addUserKnob {20 BokehKernel}
 addUserKnob {26 bokehName l "Bokeh Name:" T "Moon (269x269)"}
 addUserKnob {7 switchBokeh l "Bokeh Switch Slider" R 0 27}
